{
    "version": 2,
    "builds": [
      {
        "src": "src/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.js",
        "methods":[
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
      }
    ],
    "env": {
        "SEPOLIA_RPC_URL": "@sepolia_rpc_url",
        "EMAIL_USER": "@email_user",
        "EMAIL_PASS": "@email_pass",
        "OTP_SECRET": "@otp_secret",
        "MONGO_URI": "@mongo_uri",
        "JWT_SECRET": "@jwt_secret"
      }
  }
  